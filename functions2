#include <stdio.h>

logList* mergeLists(logList* resultList, logList* inList){
 
    logList* temp = resultList;
    
    while(resultList->next != NULL){
        resultList = resultList->next;
    }
    resultList->next = logList->next;
    return temp;
}

int listLength(logList* testList){
    int counter = 0;
    
    while(testlist != NULL){
        counter++;
        testList = testList->next;
    }
    
    return counter;
}

logList* sortList(logList* inList){
    
    int switched = 0;
    logList *ptr1;
    logList *ptr2 = NULL;
    
    char[4] year1; char[2] month1; char[2] date1; char[2] hours1; char[2] seconds1; char[2] minutes1;
    char[4] year2; char[2] month2; char[2] date2; char[2] hours1; char[2] seconds2; char[2] minutes2;
    
    
    do{
        switched = 0;
        ptr1 = inList;
        
        while(ptr1->next != ptr2) {
            if (strtol(year1) > strtol(year2)){
                switch(ptr1, ptr->next);
                switched = 1;
            }
            ptr1 = ptr1->next;
        }
        ptr2 = ptr1;
    }

void switch(logList* a, logList* b){
    char[20] temp1;
    char[20] temp2;
    char[20] temp3;
    
    temp1 = a->level; temp2 = a->timeStamp; temp3 = a->message;
    a->level = b->level; a->timeStamp = b->timeStamp; a->message = b->message;
    b->level = temp1; b->timeStamp = temp2; b->message = temp3;
    
} 
